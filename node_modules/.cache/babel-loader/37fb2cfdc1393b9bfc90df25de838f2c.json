floor(Math.random() * (100 - 1 + 1)) + 1);\n      this.updatingDonutChart.series = arr;\n    },\n\n    removeData: function() {\n      if (this.updatingDonutChart.series.length === 1) return;\n      var arr = this.updatingDonutChart.series.slice();\n      arr.pop();\n      this.updatingDonutChart.series = arr;\n    },\n\n    randomize: function() {\n      this.updatingDonutChart.series = this.updatingDonutChart.series.map(function() {\n        return Math.floor(Math.random() * (100 - 1 + 1)) + 1;\n      });\n    },\n\n    reset: function() {\n      this.updatingDonutChart.series = [44, 55, 13, 33];\n    },\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Simple Pie Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"300\"\n              dir=\"ltr\"\n              :series=\"simplePieChart.series\"\n              :options=\"simplePieChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Simple Donut Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"300\"\n              dir=\"ltr\"\n              :series=\"donutChart.series\"\n              :options=\"donutChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Updating Donut Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"300\"\n              dir=\"ltr\"\n              :series=\"updatingDonutChart.series\"\n              :options=\"updatingDonutChart.chartOptions\"\n            ></apexchart>\n\n            <div\n              class=\"d-flex align-items-start flex-wrap gap-2 justify-content-center mt-4\"\n            >\n              <button id=\"add\" class=\"btn btn-light btn-sm\" @click=\"appendData\">\n                + ADD\n              </button>\n\n              <button\n                id=\"remove\"\n                class=\"btn btn-light btn-sm\"\n                @click=\"removeData\"\n              >\n                - REMOVE\n              </button>\n\n              <button\n                id=\"randomize\"\n                class=\"btn btn-light btn-sm\"\n                @click=\"randomize\"\n              >\n                RANDOMIZE\n              </button>\n\n              <button id=\"reset\" class=\"btn btn-light btn-sm\" @click=\"reset\">\n                RESET\n              </button>\n            </div>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Monochrome Pie Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"300\"\n              dir=\"ltr\"\n              :series=\"monochromePieChart.series\"\n              :options=\"monochromePieChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Gradient Donut Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"300\"\n              dir=\"ltr\"\n              :series=\"gradientDonutChart.series\"\n              :options=\"gradientDonutChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Patterned Donut Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"300\"\n              dir=\"ltr\"\n              :series=\"patternDonutChart.series\"\n              :options=\"patternDonutChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Pie Chart with Image Fill</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"300\"\n              dir=\"ltr\"\n              :series=\"imagePieChart.series\"\n              :options=\"imagePieChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n    </div>\n  </Layout>\n</template>\n"],"sourceRoot":""}]}