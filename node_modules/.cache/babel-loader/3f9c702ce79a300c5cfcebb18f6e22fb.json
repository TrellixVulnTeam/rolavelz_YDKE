AAe,MAAf,CAAsB,QAAtB;AACF,SAJA,MAIO;AACL;AACA,UAAA,OAAO,CAAC,SAAR,CAAkB,MAAlB,CAAyB,QAAzB;AACA,UAAA,IAAI,CAAC,SAAL,CAAe,GAAf,CAAmB,QAAnB;AACF;AACD,OAbD;AAcD,KAfD;AAgBD;AA1CY,CAAf","sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\n\nexport default {\n  page: {\n    title: \"Select\",\n    meta: [{ name: \"description\", content: appConfig.description }],\n  },\n  data() {\n    return {\n      title: \"Select\",\n      items: [\n        {\n          text: \"Forms\",\n          href: \"/\",\n        },\n        {\n          text: \"Select\",\n          active: true,\n        },\n      ],\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n  },\n  mounted() {\n    const checkbox = document.getElementsByClassName(\"code-switcher\");\n    Array.from(checkbox).forEach((check) => {\n      check.addEventListener(\"change\", () => {\n        const card = check.closest(\".card\");\n        const preview = card.querySelector(\".live-preview\");\n        const code = card.querySelector(\".code-view\");\n        if (check.checked) {\n          // do this\n          preview.classList.add(\"d-none\");\n          code.classList.remove(\"d-none\");\n        } else {\n          // do that\n          preview.classList.remove(\"d-none\");\n          code.classList.add(\"d-none\");\n        }\n      });\n    });\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <div class=\"row\">\n      <div class=\"col-lg-12\">\n        <div class=\"card\">\n          <div class=\"card-header align-items-center d-flex\">\n            <h4 class=\"card-title mb-0 flex-grow-1\">Default Select</h4>\n            <div class=\"flex-shrink-0\">\n              <div\n                class=\"form-check form-switch form-switch-right form-switch-md\"\n              >\n                <label for=\"FormSelectDefault\" class=\"form-label text-muted\"\n                  >Show Code</label\n                >\n                <input\n                  class=\"form-check-input code-switcher\"\n                  type=\"checkbox\"\n                  id=\"FormSelectDefault\"\n                />\n              </div>\n            </div>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use <code>&lt;select&gt;</code> attribute with numerous options to\n              show value with choice's option.\n            </p>\n            <div class=\"live-preview\">\n              <div class=\"row\">\n                <div class=\"col-lg-6\">\n                  <select\n                    class=\"form-select mb-3\"\n                    aria-label=\"Default select example\"\n                  >\n                    <option selected>Select your Status</option>\n                    <option value=\"1\">Declined Payment</option>\n                    <option value=\"2\">Delivery Error</option>\n                    <option value=\"3\">Wrong Amount</option>\n                  </select>\n                </div>\n                <div class=\"col-lg-6\">\n                  <select\n                    class=\"form-select rounded-pill mb-3\"\n                    aria-label=\"Default select example\"\n                  >\n                    <option selected>Search for services</option>\n                    <option value=\"1\">Information Architecture</option>\n                    <option value=\"2\">UI/UX Design</option>\n                    <option value=\"3\">Back End Development</option>\n                  </select>\n                </div>\n                <div class=\"col-lg-6\">\n                  <select\n                    class=\"form-select\"\n                    aria-label=\"Disabled select example\"\n                    disabled\n                  >\n                    <option selected>Open this select menu (Disabled)</option>\n                    <option value=\"1\">One</option>\n                    <option value=\"2\">Two</option>\n                    <option value=\"3\">Three</option>\n                  </select>\n                </div>\n              </div>\n            </div>\n\n            <div class=\"d-none code-view\">\n              <pre class=\"language-markup\">\n<code> &lt;select class=&quot;form-select mb-3&quot; aria-label=&quot;Default select example&quot;&gt;\n    &lt;option selected&gt;Open this select menu&lt;/option&gt;\n    &lt;option value=&quot;1&quot;&gt;One&lt;/option&gt;\n    &lt;option value=&quot;2&quot;&gt;Two&lt;/option&gt;\n    &lt;option value=&quot;3&quot;&gt;Three&lt;/option&gt;\n&lt;/select&gt;</code>\n\n<code> &lt;select class=&quot;form-select rounded-pill mb-3&quot; aria-label=&quot;Default select example&quot;&gt;\n    &lt;option selected&gt;Search for services&lt;/option&gt;\n    &lt;option value=&quot;1&quot;&gt;Information Architecture&lt;/option&gt;\n    &lt;option value=&quot;2&quot;&gt;UI/UX Design&lt;/option&gt;\n    &lt;option value=&quot;3&quot;&gt;Back End Development&lt;/option&gt;\n&lt;/select&gt;</code>\n\n<code>&lt;select class=&quot;form-select&quot; aria-label=&quot;Disabled select example&quot; disabled&gt;\n    &lt;option selected&gt;Open this select menu (Disabled)&lt;/option&gt;\n    &lt;option value=&quot;1&quot;&gt;One&lt;/option&gt;\n    &lt;option value=&quot;2&quot;&gt;Two&lt;/option&gt;\n    &lt;option value=&quot;3&quot;&gt;Three&lt;/option&gt;\n&lt;/select&gt;</code></pre>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- end col -->\n    </div>\n\n    <div class=\"row\">\n      <div class=\"col-lg-12\">\n        <div class=\"card\">\n          <div class=\"card-header align-items-center d-flex\">\n            <h4 class=\"card-title mb-0 flex-grow-1\">Menu Size</h4>\n            <div class=\"flex-shrink-0\">\n              <div\n                class=\"form-check form-switch form-switch-right form-switch-md\"\n              >\n                <label for=\"FormSelectMenuSizing\" class=\"form-label text-muted\"\n                  >Show Code</label\n                >\n                <input\n                  class=\"form-check-input code-switcher\"\n                  type=\"checkbox\"\n                  id=\"FormSelectMenuSizing\"\n                />\n              </div>\n            </div>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use <code>multiple</code> attribute to select multiple values and\n              <code>size=\"your value\"</code> attribute to show a number of\n              choice option within a select area.\n            </p>\n            <div class=\"live-preview\">\n              <div class=\"row gy-4\">\n                <div class=\"col-lg-6\">\n                  <select\n                    class=\"form-select\"\n                    multiple\n                    aria-label=\"multiple select example\"\n                  >\n                    <option selected>\n                      Open this select menu (multiple select option)\n                    </option>\n                    <option value=\"1\">One</option>\n                    <option value=\"2\">Two</option>\n                    <option value=\"3\">Three</option>\n                  </select>\n                </div>\n                <div class=\"col-lg-6\">\n                  <select\n                    class=\"form-select\"\n                    size=\"3\"\n                    aria-label=\"size 3 select example\"\n                  >\n                    <option selected>\n                      Open this select menu (select menu size)\n                    </option>\n                    <option value=\"1\">One</option>\n                    <option value=\"2\">Two</option>\n                    <option value=\"3\">Three</option>\n                    <option value=\"4\">Four</option>\n                    <option value=\"5\">Five</option>\n                  </select>\n                </div>\n              </div>\n            </div>\n            <div class=\"d-none code-view\">\n              <pre class=\"language-markup\" style=\"height: 275px\">\n<code>&lt;select class=&quot;form-select&quot; multiple aria-label=&quot;multiple select example&quot;&gt;\n    &lt;option selected&gt;Open this select menu (multiple select option)&lt;/option&gt;\n    &lt;option value=&quot;1&quot;&gt;One&lt;/option&gt;\n    &lt;option value=&quot;2&quot;&gt;Two&lt;/option&gt;\n    &lt;option value=&quot;3&quot;&gt;Three&lt;/option&gt;\n&lt;/select&gt;</code>\n\n<code>&lt;select class=&quot;form-select&quot; size=&quot;3&quot; aria-label=&quot;size 3 select example&quot;&gt;\n    &lt;option selected&gt;Open this select menu (select menu size)&lt;/option&gt;\n    &lt;option value=&quot;1&quot;&gt;One&lt;/option&gt;\n    &lt;option value=&quot;2&quot;&gt;Two&lt;/option&gt;\n    &lt;option value=&quot;3&quot;&gt;Three&lt;/option&gt;\n    &lt;option value=&quot;4&quot;&gt;Four&lt;/option&gt;\n    &lt;option value=&quot;5&quot;&gt;Five&lt;/option&gt;\n&lt;/select&gt;</code></pre>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- end col -->\n    </div>\n\n    <div class=\"row\">\n      <div class=\"col-lg-12\">\n        <div class=\"card\">\n          <div class=\"card-header align-items-center d-flex\">\n            <h4 class=\"card-title mb-0 flex-grow-1\">Select Size</h4>\n            <div class=\"flex-shrink-0\">\n              <div\n                class=\"form-check form-switch form-switch-right form-switch-md\"\n              >\n                <label for=\"FormSelectSizing\" class=\"form-label text-muted\"\n                  >Show Code</label\n                >\n                <input\n                  class=\"form-check-input code-switcher\"\n                  type=\"checkbox\"\n                  id=\"FormSelectSizing\"\n                />\n              </div>\n            </div>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use <code>form-select-sm</code> class to set small select size and\n              <code>form-select-lg</code> class to form-select class to set\n              large select size. No such class is required for default select\n              size.\n            </p>\n            <div class=\"live-preview\">\n              <div class=\"row align-items-center g-3\">\n                <div class=\"col-lg-4\">\n                  <select\n                    class=\"form-select form-select-sm\"\n                    aria-label=\".form-select-sm example\"\n                  >\n                    <option selected>Open this select menu</option>\n                    <option value=\"1\">One</option>\n                    <option value=\"2\">Two</option>\n                    <option value=\"3\">Three</option>\n                  </select>\n                </div>\n                <div class=\"col-lg-4\">\n                  <select\n                    class=\"form-select\"\n                    aria-label=\".form-select-sm example\"\n                  >\n                    <option selected>Open this select menu</option>\n                    <option value=\"1\">One</option>\n                    <option value=\"2\">Two</option>\n                    <option value=\"3\">Three</option>\n                  </select>\n                </div>\n                <div class=\"col-lg-4\">\n                  <select\n                    class=\"form-select form-select-lg\"\n                    aria-label=\".form-select-lg example\"\n                  >\n                    <option selected>Open this select menu</option>\n                    <option value=\"1\">One</option>\n                    <option value=\"2\">Two</option>\n                    <option value=\"3\">Three</option>\n                  </select>\n                </div>\n              </div>\n            </div>\n            <div class=\"d-none code-view\">\n              <pre\n                class=\"language-markup\"\n                style=\"height: 275px\"\n              ><code>&lt;select class=&quot;form-select form-select-sm  mb-3&quot; aria-label=&quot;.form-select-sm example&quot;&gt;\n    &lt;option selected&gt;Open this select menu&lt;/option&gt;\n    &lt;option value=&quot;1&quot;&gt;One&lt;/option&gt;\n    &lt;option value=&quot;2&quot;&gt;Two&lt;/option&gt;\n    &lt;option value=&quot;3&quot;&gt;Three&lt;/option&gt;\n&lt;/select&gt;</code>\n\n<code>&lt;select class=&quot;form-select mb-3&quot; aria-label=&quot;.form-select-lg example&quot;&gt;\n    &lt;option selected&gt;Open this select menu&lt;/option&gt;\n    &lt;option value=&quot;1&quot;&gt;One&lt;/option&gt;\n    &lt;option value=&quot;2&quot;&gt;Two&lt;/option&gt;\n    &lt;option value=&quot;3&quot;&gt;Three&lt;/option&gt;\n&lt;/select&gt;</code>\n\n<code>&lt;select class=&quot;form-select form-select-lg&quot; aria-label=&quot;.form-select-lg example&quot;&gt;\n    &lt;option selected&gt;Open this select menu&lt;/option&gt;\n    &lt;option value=&quot;1&quot;&gt;One&lt;/option&gt;\n    &lt;option value=&quot;2&quot;&gt;Two&lt;/option&gt;\n    &lt;option value=&quot;3&quot;&gt;Three&lt;/option&gt;\n&lt;/select&gt;</code></pre>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n  </Layout>\n</template>\n"],"sourceRoot":""}]}