     <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"multipleSeriesHeatmapChart.series\"\n              :options=\"multipleSeriesHeatmapChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Heatmap Color Range</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"colorHeatmapChart.series\"\n              :options=\"colorHeatmapChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Heatmap - Range Without Shades</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"shadesHeatmapChart.series\"\n              :options=\"shadesHeatmapChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n  </Layout>\n</template>\n"],"sourceRoot":""}]}