eries\"\n              :options=\"stackedColumnsChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Stacked Column 100</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"columnStackedChart.series\"\n              :options=\"columnStackedChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Column with Markers</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"columnMarkersChart.series\"\n              :options=\"columnMarkersChart.chartOptions\"\n            ></apexchart>\n            <div id=\"column_markers\" class=\"apex-charts\" dir=\"ltr\"></div>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-head