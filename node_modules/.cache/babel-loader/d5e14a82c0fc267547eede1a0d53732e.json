           class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"mixedLineChart.series\"\n              :options=\"mixedLineChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Multiple Y-Axis Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"yAxisChart.series\"\n              :options=\"yAxisChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Line & Area Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"lineAreaChart.series\"\n              :options=\"lineAreaChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Line, Column & Area Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"lineColumnAreaChart.series\"\n              :options=\"lineColumnAreaChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n  </Layout>\n</template>"],"sourceRoot":""}]}