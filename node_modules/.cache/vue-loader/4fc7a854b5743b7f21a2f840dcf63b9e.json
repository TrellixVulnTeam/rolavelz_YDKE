C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACtC,CAAC;AACH,CAAC","file":"/workspace/rolavelz/src/components/EditarTarea.vue","sourceRoot":"","sourcesContent":["<script>\nimport { mapState, mapActions } from \"vuex\";\nimport _ from 'lodash'\n\nexport default {\n  computed: {\n    ...mapState(\"invoice\", {\n      tarea: (state) => _.cloneDeep(state.tarea),\n    }),\n  },\n  methods: {\n    ...mapActions(\"invoice\", [\"setTarea\", \"updateTarea\"]),\n  },\n  created() {\n    this.setTarea(this.$route.params.id);\n  },\n};\n</script>\n\n<template>\n  <div class=\"col-xl-9 m-5 col-lg-8\">\n    <div>\n      <div class=\"card\">\n        <div class=\"card-header border-0\">\n          <div class=\"row g-4\">\n            <div class=\"col-sm-auto\">\n              <div>\n                <form @submit.prevent=\"updateTarea(tarea)\">\n                  <div class=\"row\">\n                    <div class=\"col-6\">\n                      <div class=\"mb-3\">\n                        <label class=\"form-label\">First Name</label>\n                        <input\n                          type=\"text\"\n                          class=\"form-control\"\n                          placeholder=\"Enter your firstname\"\n                          v-model.trim=\"tarea.firstname\"\n                        />\n                      </div>\n                    </div>\n                    <!--end col-->\n                    <div class=\"col-6\">\n                      <div class=\"mb-3\">\n                        <label class=\"form-label\">Last Name</label>\n                        <input\n                          type=\"text\"\n                          class=\"form-control\"\n                          placeholder=\"Enter your lastname\"\n                          v-model.trim=\"tarea.lastname\"\n                        />\n                      </div>\n                    </div>\n                    <!--end col-->\n                    <div class=\"col-12\">\n                      <div class=\"mb-3\">\n                        <label for=\"compnayNameinput\" class=\"form-label\">Company Name</label>\n                        <input\n                          type=\"text\"\n                          class=\"form-control\"\n                          placeholder=\"Enter company name\"\n                          id=\"compnayNameinput\"\n                          v-model.trim=\"tarea.company\"\n                        />\n                      </div>\n                    </div>\n                    <!--end col-->\n                    <div class=\"col-6\">\n                      <div class=\"mb-3\">\n                        <label for=\"phonenumberInput\" class=\"form-label\">Phone Number</label>\n                        <input\n                          type=\"tel\"\n                          class=\"form-control\"\n                          placeholder=\"+(245) 451 45123\"\n                          id=\"phonenumberInput\"\n                          v-model.trim=\"tarea.phone\"\n                        />\n                      </div>\n                    </div>\n                    <!--end col-->\n                    <div class=\"col-6\">\n                      <div class=\"mb-3\">\n                        <label for=\"emailidInput\" class=\"form-label\">Email Address</label>\n                        <input\n                          type=\"email\"\n                          class=\"form-control\"\n                          placeholder=\"example@gamil.com\"\n                          id=\"emailidInput\"\n                          v-model.trim=\"tarea.email\"\n                        />\n                      </div>\n                    </div>\n                    <!--end col-->\n                    <div class=\"col-12\">\n                      <div class=\"mb-3\">\n                        <label for=\"address1ControlTextarea\" class=\"form-label\">Address</label>\n                        <input\n                          type=\"text\"\n                          class=\"form-control\"\n                          placeholder=\"Address 1\"\n                          id=\"address1ControlTextarea\"\n                          v-model.trim=\"tarea.address\"\n                        />\n                      </div>\n                    </div>\n                    <!--end col-->\n                    <div class=\"col-6\">\n                      <div class=\"mb-3\">\n                        <label for=\"citynameInput\" class=\"form-label\">City</label>\n                        <input\n                          type=\"text\"\n                          class=\"form-control\"\n                          placeholder=\"Enter your city\"\n                          id=\"citynameInput\"\n                          v-model.trim=\"tarea.city\"\n                        />\n                      </div>\n                    </div>\n                    <!--end col-->\n                    <div class=\"col-6\">\n                      <div class=\"mb-3\">\n                        <label for=\"ForminputState\" class=\"form-label\">State</label>\n                        <select id=\"ForminputState\" class=\"form-select\" v-model.trim=\"tarea.state\">\n                          <option selected>Choose...</option>\n                          <option>Coahuila</option>\n                          <option>Nuevo Leon</option>\n                          <option>Queretaro</option>\n                        </select>\n                      </div>\n                    </div>\n                    <!--end col-->\n                    <div class=\"col-lg-12\">\n                      <div class=\"text-end\">\n                        <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n                      </div>\n                    </div>\n                    <!--end col-->\n                  </div>\n                  <!--end row-->\n                </form>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>"]}]}